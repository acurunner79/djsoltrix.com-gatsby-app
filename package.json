{
  "name": "dj-soltrix",
  "version": "1.0.0",
  "private": true,
  "description": "DJ Soltrix",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10",
    "Chrome 96"
  ],
  "keywords": [
    "gatsby"
  ],
  "_cmd-update-all-modules": "npm run update-all-modules",
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "reinstall": "reinstall-node-modules",
    "create-global-node-modules-folder": "if not exist \"%appdata%\\npm\\node_modules\" mkdir %appdata%\\npm\\node_modules",
    "npm-i-g": "npm i npm@latest -g",
    "npm-check-i-g": "npm i npm-check@latest -g",
    "npm-check-u-l": "npm-check \"C:\\Program Files\\nodejs\\node_modules\\npm\" -y",
    "npm-check-u-g": "npm-check \"C:\\Program Files\\nodejs\\node_modules\\npm\" -y -g",
    "npm-deep-update-l": "npm update --depth 9999 --dev",
    "npm-deep-update-g": "npm update --depth 9999 --dev -g",
    "npm-cache-clear": "npm cache clear --force",
    "update-all-modules": "npm run create-global-node-modules-folder && npm run npm-i-g && npm run npm-check-i-g && npm run npm-check-u-l && npm run npm-check-u-g && npm run npm-deep-update-l && npm run npm-deep-update-g && npm run npm-cache-clear"
  },
  "dependencies": {
    "@formspree/react": "^2.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "bootstrap": "^5.1.3",
    "custom-env": "^2.0.1",
    "dotenv": "^16.0.1",
    "gatsby": "^4.15.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-config": "^1.0.7",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^2.15.1",
    "gatsby-plugin-manifest": "^4.15.1",
    "gatsby-plugin-react-helmet": "^5.15.0",
    "gatsby-plugin-sharp": "^4.15.1",
    "gatsby-plugin-styled-components": "^5.15.0",
    "gatsby-source-contentful": "^7.13.1",
    "gatsby-source-filesystem": "^4.15.0",
    "gatsby-source-graphql": "^4.15.0",
    "gatsby-source-instagram-all": "^5.2.1",
    "gatsby-source-rest-api": "^0.2.3",
    "gatsby-source-youtube-playlist": "^1.0.2",
    "gatsby-source-youtube-v3": "^3.0.2",
    "gatsby-transformer-playlists": "^0.3.0",
    "gatsby-transformer-remark": "^5.15.1",
    "gatsby-transformer-sharp": "^4.15.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "react-player": "^2.10.1",
    "styled-components": "^5.3.5",
    "youtube-playlist-info": "^1.1.2"
  },
  "devDependencies": {
    "reinstall-node-modules": "^2.0.0"
  }
}
